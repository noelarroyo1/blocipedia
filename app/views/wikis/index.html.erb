<h1> Welcome to Blocipedia! </p>

<h2>Wikis</h2>

<% @wiki.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to wiki.title, wiki_path(wiki.id) %>
      </h4>
    </div>
  </div>
<% end %>

<div class="col-lg" style="margin-top: 1.2em;">
  <%= link_to "New", new_wiki_path, class: 'btn btn-primary' %>
</div>
